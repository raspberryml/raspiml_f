#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/svelte-check@2.10.3_postcss@8.4.49_sass@1.82.0_svelte@3.59.2/node_modules/svelte-check/bin/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/svelte-check@2.10.3_postcss@8.4.49_sass@1.82.0_svelte@3.59.2/node_modules/svelte-check/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/svelte-check@2.10.3_postcss@8.4.49_sass@1.82.0_svelte@3.59.2/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/svelte-check@2.10.3_postcss@8.4.49_sass@1.82.0_svelte@3.59.2/node_modules/svelte-check/bin/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/svelte-check@2.10.3_postcss@8.4.49_sass@1.82.0_svelte@3.59.2/node_modules/svelte-check/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/svelte-check@2.10.3_postcss@8.4.49_sass@1.82.0_svelte@3.59.2/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svelte-check" "$@"
else
  exec node  "$basedir/../../bin/svelte-check" "$@"
fi
