#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@3.2.11_sass@1.82.0_/node_modules/@sveltejs/kit/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@3.2.11_sass@1.82.0_/node_modules/@sveltejs/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@3.2.11_sass@1.82.0_/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@3.2.11_sass@1.82.0_/node_modules/@sveltejs/kit/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@3.2.11_sass@1.82.0_/node_modules/@sveltejs/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/@sveltejs+kit@1.0.0-next.589_svelte@3.59.2_vite@3.2.11_sass@1.82.0_/node_modules:/home/laisa/raspiml/golang/tts/tts_wails/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-kit.js" "$@"
else
  exec node  "$basedir/../../svelte-kit.js" "$@"
fi
